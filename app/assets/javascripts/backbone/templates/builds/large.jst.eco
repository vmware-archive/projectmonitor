<article class="build large <%= @status %>">
<h1 class="name">
  <%= @name %>
</h1>
<% if !@aggregate: %>
  <aside class='history'>
    <div>
      <span class="last-build"><%= @last_build %></span>
      <ol class="statuses">
        <% for status in @statuses: %>
          <li class="<%= if status then 'success' else 'failure' %>">
            <a></a>
          </li>
        <% end %>
      </ol>
    </div>
  </aside>
<% end %>
</article>
